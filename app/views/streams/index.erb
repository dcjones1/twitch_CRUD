<table>
  <thead>
    <tr>
      <% @streams.first.attributes.each do |attr| %>
      <th><%= attr[0].titleize %></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% @streams.each do |stream| %>
    <tr>
      <% stream.attributes.each do |attr| %>
      <td><%= stream[attr[0]] %></td>
      <% end %>
    </tr>
    <% end %>
  </tbody>
</table>
