<table>
 <thead>
   <tr>
     <% @users.first.attributes.each do |attr| %>
     <th><%= attr[0].titleize %></th>
     <% end %>
   </tr>
 </thead>
 <tbody>
   <% @users.each do |song| %>
   <tr>
     <% song.attributes.each do |attr| %>
     <td><%= song[attr[0]] %></td>
     <% end %>
   </tr>
   <% end %>
 </tbody>
</table>
